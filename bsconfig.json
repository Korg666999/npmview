{
  "name": "npmview",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true,
      "suffix": ".bs.js"
    }
  ],
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "rescript-webapi",
    "rescript-blueprint",
    "rescript-struct"
  ],
  "jsx": { "version": 4 },
  "bsc-flags": ["-open RescriptCore -open RescriptStruct"]
}
